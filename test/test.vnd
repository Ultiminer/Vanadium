
struct Test{
mut f32 x; 
mut f32 y; 
};


mut fn void fibonacci(u32 a, u32 b, copy u32 N)
{
    print(a);
    if(N>0)fibonacci(b,a+b,N-1);
}

fn dot_prod(Test test)
{
    return test.x*test.y;
}
mut fn read(str path)
{
    mut str data="";
    mut inFile in{path,ios::binary};
    mut str line="";
    while(get_line(in, line))
    {
        data+=line; 
    }

    return line; 
}


