Linear Algebra for vanadium HAHAHAHAHA
*/

CONST f32 PI=3.141f; 

struct Vec2D{
mut f32 x; 
mut f32 y; 
};

fn Vec2D add(Vec2D a, Vec2D b)
{
    return {a.x+b.x,a.y+b.y};
}
fn Vec2D sub(Vec2D a, Vec2D b)
{
    return {a.x-b.x,a.y-b.y};
}
fn Vec2D prod(Vec2D a, Vec2D b)
{
    return {a.x*b.x,a.y*b.y};
}
fn Vec2D div(Vec2D a, Vec2D b)
{
    return {a.x/b.x,a.y/b.y};
}
fn f32 dot(Vec2D a, Vec2D b)
{
    return a.x*b.x+a.y*b.y;
}